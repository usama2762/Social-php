<script>
	
	blueimp.Gallery.prototype.textFactory = function (obj, callback) {
		var $element = $('<div>')
		.addClass('text-content')
		.attr('title', obj.title);
		$.get(obj.href)
        .done(function (result) {
            $element.html(result);
            callback({
                type: 'load',
                target: $element[0]
			});
		})
        .fail(function () {
            callback({
                type: 'error',
                target: $element[0]
			});
		});
		return $element[0];
	};
	
	var flag=1;
    
	$("a[data-target=#servicesModal]").click(function (e) {
		e.preventDefault();
		//if(flag==1){
		if($(this).attr("data-title")!="#")
		{
		var gallery=blueimp.Gallery([		
					{title: 'Take care of fix-it lists',href: 'detail-modal.php?page=services/take-care-of-fix-it-lists',type: 'text/html'},{title: 'Install and replace appliances and stuff',href: 'detail-modal.php?page=services/install-and-replace-appliances-and-stuff',type: 'text/html'},{title: 'Troubleshoot most problems in your home',href: 'detail-modal.php?page=services/troubleshoot-most-problems-in-your-home',type: 'text/html'},{title: 'Overall general home maintenance',href: 'detail-modal.php?page=services/overall-general-home-maintenance',type: 'text/html'},{title: 'General To-Do-List',href: 'detail-modal.php?page=services/general-to-do-list',type: 'text/html'},{title: '(Water & Pet Damage/Cracks)',href: 'detail-modal.php?page=services/water-pet-damage-cracks',type: 'text/html'},{title: 'Paint & Stain Doors/Trim/Moldings',href: 'detail-modal.php?page=services/paint-stain-doors-trim-moldings',type: 'text/html'},{title: 'dfhndfghdgh',href: 'detail-modal.php?page=services/dfhndfghdgh',type: 'text/html'},{title: 'dfhndfghdgh',href: 'detail-modal.php?page=services/dfhndfghdgh',type: 'text/html'},{title: 'Exterior Leak Repair Specialist',href: 'detail-modal.php?page=services/exterior-leak-repair-specialist',type: 'text/html'},{title: '(Water & Pet Damage/Cracks)',href: 'detail-modal.php?page=services/water-pet-damage-cracks',type: 'text/html'},{title: 'Replace old plumbing with new plumbing under sinks, etc',href: 'detail-modal.php?page=services/replace-old-plumbing-with-new-plumbing-under-sinks-etc',type: 'text/html'},{title: 'Change switched lights to dimmers.',href: 'detail-modal.php?page=services/change-switched-lights-to-dimmers',type: 'text/html'},{title: 'Complete Door Lube',href: 'detail-modal.php?page=services/complete-door-lube',type: 'text/html'},				], $('#blueimp-gallery').data());
			//flag=0;
			//}
			if($(this).attr("name")!=0){
				gallery.slide($(this).attr("name"));
			}
		}
	});
	$(".anchorLink").click(function (e) {
	gallery.close();
	});
	
	
	// Filter 
    $(document).on("click", ".filter", function (event) {
        (function ($) {
            $('.filter').keyup(function () {
                var rex = new RegExp($(this).val(), 'i');
                $('.searchable li').hide();
                $('.searchable li').filter(function () {
                    return rex.test($(this).text());
				}).show();
			})
		}(jQuery));
	});
    
</script>